    //   SPDX-license-identifier-Apache-2.0
    default_applicable_licenses: ["system_libvintf_license"],
}

cc_defaults {
    name: "use_libaidlvintf_gtest_helper_static",
    static_libs: [
        "libaidlvintf_gtest_helper",
        "libbase",
        "libz",
        "libselinux",
        "libtinyxml2",
        "libvintf",
