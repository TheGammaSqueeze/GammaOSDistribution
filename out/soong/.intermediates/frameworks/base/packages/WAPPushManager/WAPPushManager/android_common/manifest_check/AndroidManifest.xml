<manifest package="com.android.smspush" xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-sdk android:minSdkVersion="32" android:targetSdkVersion="32"/>

    <permission android:name="com.android.smspush.WAPPUSH_MANAGER_BIND" android:protectionLevel="signatureOrSystem"/>

    <uses-permission android:name="android.permission.QUERY_ALL_PACKAGES"/>

    <original-package android:name="com.android.smspush"/>
    <application android:allowClearUserData="false" android:directBootAware="true" android:extractNativeLibs="true" android:usesNonSdkApi="true">
        <service android:exported="true" android:name=".WapPushManager" android:permission="com.android.smspush.WAPPUSH_MANAGER_BIND">
            <intent-filter>
                <action android:name="com.android.internal.telephony.IWapPushManager"/>
            </intent-filter>
        </service>
    </application>


</manifest>
