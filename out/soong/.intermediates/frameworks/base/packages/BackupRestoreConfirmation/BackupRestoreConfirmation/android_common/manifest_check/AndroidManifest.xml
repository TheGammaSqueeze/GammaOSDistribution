<manifest package="com.android.backupconfirm" xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-sdk android:minSdkVersion="32" android:targetSdkVersion="32"/>

    <uses-permission android:name="android.permission.BACKUP"/>
    <uses-permission android:name="android.permission.CRYPT_KEEPER"/>

    <application android:allowBackup="false" android:allowClearUserData="false" android:extractNativeLibs="true" android:permission="android.permission.CONFIRM_FULL_BACKUP" android:usesNonSdkApi="true">

        <activity android:excludeFromRecents="true" android:exported="true" android:launchMode="singleTop" android:name=".BackupRestoreConfirmation" android:title="" android:windowSoftInputMode="stateAlwaysHidden">
        </activity>
    </application>
</manifest>
