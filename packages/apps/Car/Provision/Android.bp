    default_applicable_licenses: ["Android-Apache-2.0"],
}

android_app {
    name: "CarProvision",
    srcs: ["**/*.java"],
    libs: [
        "android.car-stubs",
        "android.car-system-stubs",
    ],
    static_libs: [
        "car-setup-wizard-lib",
    ],
    platform_apis: true,
    system_ext_specific: true,
    certificate: "platform",
    privileged: true,
    required: ["allowed_privapp_com.android.car.provision"],
    overrides: [
        "Provision", // There can be only one!
    ],
}
