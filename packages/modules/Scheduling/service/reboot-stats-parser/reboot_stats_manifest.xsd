<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xs="http://www.w3.org/2001/XMLSchema"
            version="1.0">
    <xsd:element name="rebootStats">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="startTimeMs" type="xsd:long"/>
                <xsd:element name="readyTimeMs" type="xsd:long"/>
                <xsd:element name="timesBlockedByInteractivity" type="xsd:int"/>
                <xsd:element name="timesBlockedByAppActivity" type="xsd:int"/>
                <xsd:element name="timesBlockedBySubsystems" type="xsd:int"/>
                <xsd:element name="timeUntilNextInteractionMs" type="xsd:long"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
</xsd:schema>