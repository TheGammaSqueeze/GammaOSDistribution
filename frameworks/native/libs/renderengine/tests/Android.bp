    include_dirs: [
        "external/skia/src/gpu",
    ],
    static_libs: [
        "libgmock",
        "librenderengine",
        "librenderengine_mocks",
    ],

    shared_libs: [
        "libbase",
        "libcutils",
        "libEGL",
        "libGLESv2",
        "libgui",
        "liblog",
        "libnativewindow",
        "libprocessgroup",
        "libsync",
        "libui",
        "libutils",
    ],
