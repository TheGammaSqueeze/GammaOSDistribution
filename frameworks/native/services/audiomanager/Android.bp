    default_applicable_licenses: ["frameworks_native_license"],
}

cc_library_shared {
    name: "libaudiomanager",

    srcs: [
        "IAudioManager.cpp",
    ],

    shared_libs: [
        "libutils",
        "libbinder",
        "liblog",
