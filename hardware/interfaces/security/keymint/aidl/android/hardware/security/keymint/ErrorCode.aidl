    INVALID_ISSUER_SUBJECT = -83,
    BOOT_LEVEL_EXCEEDED = -84,
    HARDWARE_NOT_YET_AVAILABLE = -85,

    UNIMPLEMENTED = -100,
    VERSION_MISMATCH = -101,

    UNKNOWN_ERROR = -1000,

    // Implementer's namespace for error codes starts at -10000.
}
