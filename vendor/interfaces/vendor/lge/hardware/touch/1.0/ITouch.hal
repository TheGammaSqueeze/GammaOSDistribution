package vendor.lge.hardware.touch@1.0;

interface ITouch {
	setImeState(ImeStatus state) generates (int32_t err);
	setCoverSensitivity(bool enable) generates (int32_t err);
	setGloveSensitivity(bool enable) generates (int32_t err);
	getPenSupport() generates (int32_t, bool support);
	setAppData(AppInfo data) generates (int32_t err);
	setCallState(CallStatus state) generates (int32_t err);
	setLockSrceenState(LockScreenStatus state) generates (int32_t err);
	setSpLinkState(bool enable) generates (int32_t e rr);
	setVrState(bool enable) generates (int32_t err);
	setSensingState(bool enable) generates (int32_t err);
	getGripSuppressionState() generates (int32_t err, bool support);
	setGripSuppressionState(bool enable) generates (int32_t err);
	getFirmwareVersion() generates (int32_t err, string version);
	doFirmwareUpgrade() generates (int32_t err);
	doSelfDiagnosis() generates (int32_t err, string result);
	setDebugToolState(bool enable) generates (int32_t err);
	getDebugToolState() generates (int32_t err, bool enable);
	setDebugOptionState(int32_t state) generates (int32_t err);
	getDebugOptionState() generates (int32_t err, int32_t state);
	setAutoTune(string command) generates (int32_t err);
	getAutoTune() generates (int32_t err, string result);
	doAftSelfDiagnosis() generates (int32_t err, string result);
	setFontEffect(WatchFontEffectConfig effect) generates (int32_t err);
	getFontEnable() generates (int32_t err, bool enable);
	setFontEnable(bool enable) generates (int32_t err);
	setFontPosition(WatchFontPositionConfig position) generates (int32_t err);
	setFontProperty(WatchFontPropertyConfig property) generates (int32_t err);
	setTimeSync(WatchTimeSyncConfig time) generates (int32_t err);
	setFontData(vec<int8_t> fontdata) generates (int32_t err);
};
